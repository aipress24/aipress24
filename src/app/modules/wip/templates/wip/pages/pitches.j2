{% extends "wip/layout/_base.j2" %}

{% block body_content %}
  <div class="border-b border-gray-200">
    <nav class="mt-2 -mb-px flex space-x-8" aria-label="Tabs">
      {% for tab in tabs %}
        {% if tab.active %}
          {% set class="border-indigo-500 text-indigo-600 whitespace-nowrap py-4 px-1 border-b-2 font-medium text-sm" %}
        {% else %}
          {% set class="border-transparent text-gray-500 hover:text-gray-700 hover:border-gray-200 whitespace-nowrap py-4 px-1 border-b-2 font-medium text-sm" %}
        {% endif %}
        <a href="{{ tab.url }}"
           class="{{ class }}">
          {{ tab.label }}
          <span
              class="bg-gray-100 text-gray-900 hidden ml-2 py-0.5 px-2.5 rounded-full text-xs font-medium md:inline-block">{{ tab.count }}</span>
        </a>
      {% endfor %}
    </nav>
  </div>

  <!-- Stacked list -->
  <ul role="list" class="mt-5 border-t border-gray-200 divide-y divide-gray-200 sm:mt-0 sm:border-t-0">
    <li>
      <a href="#" class="group block">
        <div class="flex items-center py-5 px-4 sm:py-6 sm:px-0">
          <div class="min-w-0 flex-1 flex items-center">
            <div class="flex-shrink-0">
              <img class="h-12 w-12 rounded-full group-hover:opacity-75"
                   src="https://images.unsplash.com/photo-1502685104226-ee32379fefbe?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=facearea&facepad=2&w=256&h=256&q=80"
                   alt="">
            </div>
            <div class="min-w-0 flex-1 px-4 md:grid md:grid-cols-2 md:gap-4">
              <div>
                <p class="text-sm font-medium text-indigo-600 truncate">Emily Selman</p>
                <p class="mt-2 flex items-center text-sm text-gray-500">
                  {{ icon("mail", size=5, class="flex-shrink-0 mr-1.5 text-gray-400 h-5 w-5") }}
                  <span class="truncate">emilyselman@example.com</span>
                </p>
              </div>

              <div class="hidden md:block">
                <div>
                  <p class="text-sm text-gray-900">
                    Créé le
                    <time datetime="2020-07-01T15:34:56">January 7, 2020</time>
                  </p>
